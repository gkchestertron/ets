<div class="panel panel-default">
    <div class="panel-heading">
        <h3><%= group.get('description') %></h3>
    </div>
    <div class="panel-body">
<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th data-comparator="bib_number"><div class="th-title">Bib #</div><div class="sort-arrows"><span class="glyphicon glyphicon-arrow-up"></span><span class="glyphicon glyphicon-arrow-down"></span></div></th>
            <th data-comparator="last_name"><div class="th-title">Name</div><div class="sort-arrows"><span class="glyphicon glyphicon-arrow-up"></span><span class="glyphicon glyphicon-arrow-down"></span></div></th>
            <th data-comparator="gender"><div class="th-title">Gender</div><div class="sort-arrows"><span class="glyphicon glyphicon-arrow-up"></span><span class="glyphicon glyphicon-arrow-down"></span></div></th>
            <th data-comparator="age"><div class="th-title">Age</div><div class="sort-arrows"><span class="glyphicon glyphicon-arrow-up"></span><span class="glyphicon glyphicon-arrow-down"></span></div></th>
            <th data-comparator="start_time"><div class="th-title">Start</div><div class="sort-arrows"><span class="glyphicon glyphicon-arrow-up"></span><span class="glyphicon glyphicon-arrow-down"></span></div></th>
            <% race.get('split_templates').each(function (template, i) { %>
            <th data-comparator="<%= template.get('label') %>"><div class="th-title"><%= template.get('label') %></div><div class="sort-arrows"><span class="glyphicon glyphicon-arrow-up"></span><span class="glyphicon glyphicon-arrow-down"></span></div></th>
            <% }); %>
            <th data-comparator="finish_time"><div class="th-title">Finish</div><div class="sort-arrows"><span class="glyphicon glyphicon-arrow-up"></span><span class="glyphicon glyphicon-arrow-down"></span></div></th>
            <th data-comparator="time"><div class="th-title">Time</div><div class="sort-arrows"><span class="glyphicon glyphicon-arrow-up"></span><span class="glyphicon glyphicon-arrow-down"></span></div></th>
            <th data-comparator="age_rank"><div class="th-title">Age Rank</div><div class="sort-arrows"><span class="glyphicon glyphicon-arrow-up"></span><span class="glyphicon glyphicon-arrow-down"></span></div></th>
            <th data-comparator="overall_rank"><div class="th-title">Overall Rank</div><div class="sort-arrows"><span class="glyphicon glyphicon-arrow-up"></span><span class="glyphicon glyphicon-arrow-down"></span></div></th>
        </tr>
    </thead>
    <tbody>
        <% entries.each(function (entry) { %>
        <tr>
            <td><%= entry.get('bib_number') %></td>
            <td><a href="entries/<%= entry.get('first_name') %>_<%= entry.get('last_name') %>"> <%= entry.get('first_name') + ' ' + entry.get('last_name') %></a></td>
            <td><%= entry.get('gender') %></td>
            <td><%= entry.get('age') %></td>
            <td><%= entry.get('start_time') %></td>
            <% entry.get('splits').each(function (split) { %>
            <td><%= split.get('length') %></td>
            <% }); %>
            <td><%= entry.get('finish_time') %></td>
            <td><%= entry.get('time') %></td>
            <td><%= entry.get('age_rank') %></td>
            <td><%= entry.get('overall_rank') %></td>
        </tr>
        <% }); %>
</table>
</div>
</div>
